generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  email     String   @unique @db.VarChar(100)
  password  String
  firstName String   @db.VarChar(100)
  lastName  String   @db.VarChar(100)
}
